  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:webpack": "next dev",
    "dev:env": "node -r dotenv/config node_modules/.bin/next dev --turbopack dotenv_config_path=../credentials/.env.local",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "build:webpack": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",

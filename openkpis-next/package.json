    "clsx": "^2.1.0",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.309.0",
    "next": "^15.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.2.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
